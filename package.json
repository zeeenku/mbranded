{
  "name": "mbranded",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run route-watcher\" \"next dev\" --names \"routes,next\" --prefix-colors \"blue,green\" --kill-others",
    "dev:next": "next dev",
    "dev:watch": "concurrently \"pnpm run page-watcher\" \"pnpm run dev:next\" --names \"watcher,next\" --prefix-colors \"cyan,green\"",
    "route-watcher": "tsx scripts/route-watcher.ts",
    "page-watcher": "tsx scripts/page-watcher.ts",
    "generate:routes": "tsx scripts/generate-routes.ts",
    "build": "next build",
    "start": "next start",
    "cli": "tsx scripts/kaizar/index.ts",
    "prepare": "husky",
    "lint": "eslint",
    "format": "prettier --check .",
    "precommit": "echo 'tessst'"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "dependencies": {
    "@polinater/redis-event-bus": "^4.0.0",
