    "@trpc/client": "^11.8.0",
    "@trpc/next": "^11.8.0",
    "@trpc/react-query": "^11.8.0",
    "@trpc/server": "^11.8.0",
    "ajv": "^8.17.1",
    "commander": "^14.0.2",
    "cqrs-eda": "^1.6.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "eventemitter3": "^5.0.1",
    "next": "16.0.10",
    "oxide.ts": "^1.1.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prompts": "^2.4.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-query": "^3.39.3",
    "redis": "^5.10.0",
    "zod": "^4.2.1"
